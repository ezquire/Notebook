<%- include("../includes/header.ejs", {title:"Employees - Booked Artist"}); %>
<div class="page-header">
    <h1>Employees Who Booked an Artist</h1>
</div>
<br />

<table class="table table-striped">
    <thead>
    <tr><th>Name</th><th>Phone</th></tr>
    </thead>
    <tbody>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/employee/?employee_id=<%= result[i].employee_id %>"><%= result[i].fname %> <%= result[i].lname %></a></td>
        <td><%= result[i].phone %></td>
    </tr>
    <% } %>
    </tbody>

</table>
<%- include("../includes/footer.ejs"); %>